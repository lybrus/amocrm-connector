{
    "name": "amocrm-connector",
    "description": "AmoCRM api connector for Node.js",
    "version": "0.3.0",
    "repository": "git@github.com:lybrus/amocrm-connector.git",
    "homepage": "git@github.com:lybrus/amocrm-connector.git",
