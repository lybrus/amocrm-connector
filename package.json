        "babel-plugin-inline-json-import": "^0.3.2",
        "babel-plugin-root-import": "^6.6.0",
        "cypress": "^9.1.1",
        "dotenv-cli": "^4.1.1",
        "jest": "^27.4.4",
        "koa": "^2.13.4",
        "koa-body": "^4.2.0",
        "koa-router": "^10.1.1",
        "localtunnel": "^2.0.2",
        "ts-node": "^10.5.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.6.0-dev.20220211"
    },
    "scripts": {
        "jest": "dotenv -e .env.test jest",
        "clean": "rm -rf dist",
        "prebuild": "yarn clean",
        "build": "tsc",
        "cypress:open": "dotenv -e .env.test cypress open",
        "cypress": "dotenv -e .env.test cypress run",
        "serve:auto": "babel-node -- testing/server.js",
        "serve": "dotenv -e .env.test yarn serve:auto",
        "tunnel:auto": "yarn run lt -p $SERVER_PORT -s $TUNNEL_SUBDOMAIN",
        "tunnel": "dotenv -e .env.test yarn tunnel:auto",
        "test": "yarn cypress",
        "example-chat": "dotenv -e .env.test ts-node-dev examples/chat.ts"
    }
